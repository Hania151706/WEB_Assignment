{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.vue",
    ".nuxt/**/*.ts",
    ".nuxt/**/*.d.ts"
  ],
  "vueCompilerOptions": {
    "target": 3,
    "plugins": [
      "@vue/language-plugin-pug",
      "@vue/language-plugin-typescript"
    ]
  }
}
